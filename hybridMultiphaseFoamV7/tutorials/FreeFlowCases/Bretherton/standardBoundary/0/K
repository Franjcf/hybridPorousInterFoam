FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 0 0 0 0 0];

internalField   uniform 2e-20;

boundaryField
{
    left
    {
        type            zeroGradient;
   }

    right
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    FrontAndBack
    {
        type            empty;
    }
}

